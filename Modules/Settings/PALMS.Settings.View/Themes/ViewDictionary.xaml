<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewModel="clr-namespace:PALMS.Settings.ViewModel;assembly=PALMS.Settings.ViewModel"
                    xmlns:view="clr-namespace:PALMS.Settings.View"
                    xmlns:appSettings="clr-namespace:PALMS.Settings.ViewModel.AppSettings;assembly=PALMS.Settings.ViewModel"
                    xmlns:appSettings1="clr-namespace:PALMS.Settings.View.AppSettings"
                    xmlns:laundryDetails="clr-namespace:PALMS.Settings.ViewModel.LaundryDetails;assembly=PALMS.Settings.ViewModel"
                    xmlns:laundryDetails1="clr-namespace:PALMS.Settings.View.LaundryDetails">

    <DataTemplate DataType="{x:Type viewModel:SettingsViewModel}">
        <view:SettingsView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModel:TabsViewModel}">
        <view:TabsView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type appSettings:AppSettingsViewModel}">
        <appSettings1:AppSettingsView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type laundryDetails:VendorDetailsViewModel}">
        <laundryDetails1:LaundryDetailsView/>
    </DataTemplate>
</ResourceDictionary>
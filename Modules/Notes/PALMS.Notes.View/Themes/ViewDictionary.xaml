<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewModel="clr-namespace:PALMS.Notes.ViewModel;assembly=PALMS.Notes.ViewModel"
                    xmlns:view="clr-namespace:PALMS.Notes.View"
                    xmlns:window="clr-namespace:PALMS.Notes.View.Window"
                    xmlns:viewModel1="clr-namespace:PALMS.Notes.ViewModel.Window;assembly=PALMS.Notes.ViewModel">


    <DataTemplate DataType="{x:Type viewModel:NotesViewModel}">
        <view:NotesView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModel:DeliveryNoteViewModel}">
        <view:DeliveryNoteView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModel1:AddLinenViewModel}">
        <window:AddLinenWindow/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModel1:AddLinenListViewModel}">
        <window:AddLinenListWindow/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModel1:DepartmentSelectionViewModel}">
        <window:DepartmentSelectionWindow/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModel1:NoteLinenReplacementViewModel}">
        <window:NoteLinenReplacementWindow/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModel1:PrintCopyNumberViewModel}">
        <window:PrintCopyNumberView/>
    </DataTemplate>


</ResourceDictionary>
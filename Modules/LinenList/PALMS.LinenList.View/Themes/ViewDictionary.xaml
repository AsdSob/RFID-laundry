<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewModel="clr-namespace:PALMS.LinenList.ViewModel;assembly=PALMS.LinenList.ViewModel"
                    xmlns:view="clr-namespace:PALMS.LinenList.View"
                    xmlns:windows="clr-namespace:PALMS.LinenList.ViewModel.Windows;assembly=PALMS.LinenList.ViewModel"
                    xmlns:view1="clr-namespace:PALMS.LinenList.View.Windows">

    <DataTemplate DataType="{x:Type viewModel:LinenListViewModel}">
        <view:LinenListView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModel:LinensListViewModel}">
        <view:LinensListView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModel:LeasingLinenViewModel}">
        <view:LeasingLinenWindow/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModel:SelectCopySourceViewModel}">
        <view:SelectCopySourceWindow/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type windows:ChangeNoteLinenViewModel}">
        <view1:ChangeNoteLinenView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type windows:UnUsedLinenViewModel}">
        <view1:UnUsedLinenView/>
    </DataTemplate>

</ResourceDictionary>
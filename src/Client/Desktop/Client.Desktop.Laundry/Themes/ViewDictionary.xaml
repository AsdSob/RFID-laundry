<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewModels="clr-namespace:Client.Desktop.ViewModels;assembly=Client.Desktop.ViewModels"
                    xmlns:views="clr-namespace:Client.Desktop.Laundry.Views"
                    xmlns:views1="clr-namespace:Client.Desktop.Laundry.Views.Operation"
                    xmlns:content="clr-namespace:Client.Desktop.ViewModels.Content;assembly=Client.Desktop.ViewModels"
                    xmlns:operation="clr-namespace:Client.Desktop.ViewModels.Content.Operation;assembly=Client.Desktop.ViewModels">

    <DataTemplate DataType="{x:Type viewModels:MenuViewModel}">
        <views:MenuView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type content:DataViewModel}">
        <views:DataView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type content:AddViewModel}">
        <views:AddView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type operation:UniformConveyorViewModel}">
        <views1:UniformConveyorView/>
    </DataTemplate>

</ResourceDictionary>
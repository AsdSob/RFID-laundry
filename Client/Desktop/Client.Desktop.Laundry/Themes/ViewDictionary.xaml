<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewModels="clr-namespace:Client.Desktop.ViewModels;assembly=Client.Desktop.ViewModels"
                    xmlns:views="clr-namespace:Client.Desktop.Laundry.Views"
                    xmlns:content="clr-namespace:Client.Desktop.ViewModels.Content;assembly=Client.Desktop.ViewModels"
                    xmlns:contentModel="clr-namespace:Client.Desktop.ViewModels.Content.Master;assembly=Client.Desktop.ViewModels"
                    xmlns:master="clr-namespace:Client.Desktop.Laundry.Views.Master">

    <DataTemplate DataType="{x:Type viewModels:MenuViewModel}">
        <views:MenuView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type content:AddViewModel}">
        <views:AddView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type contentModel:ClientViewModel}">
        <master:ClientView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type contentModel:StaffViewModel}">
        <master:StaffView/>
    </DataTemplate>

</ResourceDictionary>